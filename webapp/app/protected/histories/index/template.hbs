<div class="section">
  <h4>
    History 
  </h4>
  <p class="color-primary indication">History gives you information regarding your user's sessions</p>
  <div class='content-wrapper'>

    {{#if model}}
      <table class="table">
        <thead>
          <tr>
            <th>User</th>
            <th>Application</th>
            <th>Start</th>
            <th>End</th>
          </tr>
        </thead>
        <tbody>
            {{#each model as |history|}}
              {{log history.user}}
              <tr>
                <td>{{history.user.firstName}} {{history.user.lastName}}</td>
                <td>{{history.application.displayName}}</td>
                <td>{{history.startDate}}</td>
                <td>{{history.endDate}}</td>
              </tr>
            {{/each}}
        </tbody>
      </table>
    {{else}}
      No data available
    {{/if}}
    <div class="clearfix"></div>
  </div>

</div>
