{
    "Role" : "proxy",
    "Debug" : true,
    "LogFilePath" : "/var/log/haptic.log",
    "CommonBaseDir" : "/opt/build/backend/src/nanocloud.com/bin/haptic",
    "Port" : "8081",

    "AdminUser": {
        "Email": "admin@nanocloud.com",
        "Password": "admin"
    },

    "Database" : {
        "ConnectionString" : "users.db",
        "type" : "bolt"
    },

    "Proxy" : {
        "FrontEndRootDir" : "/opt/build/frontend/website",

        "MaxNumRegistrations" : 10,
        "MaxNumAccounts" : 9,
        "NumRetries" : 3,
        "SleepDurationInSecs" : 20
    },

    "AppServer": {
      "User" : "Administrator",
      "Server" : "<execution-manager-adress>",
      "ExecutionServers" : [
        "<execution-environment-adress>"
      ],
      "SSHPort": 22,
      "RDPPort": 3389,
      "Password": "Nanocloud123+",
      "WindowsDomain": "intra.localdomain.com",
      "XMLConfigurationFile": "/etc/guacamole/noauth-config.xml"
    },

    "Plugins" : {
        "History" : {
            "ConnectionString": "/tmp/stats.db",
            "DatabaseName": "history"
        },
        "Ldap" : {
            "serverUrl": "ldaps://XX.XX.XX.XX",
            "username": "CN=Administrator,CN=Users,DC=intra,DC=localdomain,DC=com",
            "password": "Nanocloud123+",
            "scriptsDir": "/opt/build/backend/src/nanocloud.com/plugins/"
        }
    }
}

